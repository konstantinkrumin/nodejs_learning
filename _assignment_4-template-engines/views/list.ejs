<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/user.css" />
</head>

<body>
    <%- include('includes/navigation.ejs') %>
    <main>
        <% if (users.length > 0) { %>
            <% for(let user in users) { %>
                <p><%= user.name %> </p>
            <% } %> 
        <% } else { %>
            <h1>No users found</h1>
        <% } %>
    </main>
    <%- include('includes/end.ejs') %>
    